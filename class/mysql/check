insert into tt values ('b');
insert into tt values ('c');
select * from tt;
commit;
insert into tt values ('d');
rollback;
select * from tt;
delete from tt;
start transaction;
insert into tt values ('a');
insert into tt values ('b');
savepoint sb;
insert into tt values ('c');
insert into tt values ('d');
savepoint sd;
insert into tt values ('e');
insert into tt values ('f');
select * from tt;
rollback to sd;
select * from tt;
rollback to sb;
select * from tt;
commit;
