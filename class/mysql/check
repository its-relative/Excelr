DROP TABLE IF EXISTS `movies`;
CREATE TABLE `movies` (
  `id` int DEFAULT NULL,
  `title` varchar(50) DEFAULT NULL,
  `category` varchar(25) DEFAULT NULL
) ;
CREATE TABLE `movies` (   `id` int DEFAULT NULL,   `title` varchar(50) DEFAULT NULL,   `category` varchar(25) DEFAULT NULL );
