SELECT
    fname,lname
    salary,
    NTH_VALUE(fname, 3) OVER  (
        ORDER BY salary DESC
    ) third_highest_salary
FROM
    employee;
SELECT     fname,lname     salary,     NTH_VALUE(fname, 3) OVER  (         ORDER BY salary DESC     ) third_highest_salary FROM     employee;
