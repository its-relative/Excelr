insert into tt values('a'),('b');
select * from tt;
rollback;
select * from tt;
insert into tt values('a');
select * from tt;
rollback;
select * from tt;
delete from tt;
select * from tt;
start transaction;
insert into tt values('a');
insert into tt values('b');
savepont sb;
savepoint sb;
insert into tt values('c');
insert into tt values('d');
savepoint sd;
insert into tt values('e');
insert into tt values('f');
select * from tt;
rollback to sd;
select * from tt;
rollback to sb;
select * from tt;
commit;
insert into tt values('f');
