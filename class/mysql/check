CREATE TABLE `cats` (
  `name` text,
  `breed` text,
  `weight` double DEFAULT NULL,
  `color` text,
  `age` int DEFAULT NULL
) ;
CREATE TABLE `cats` (   `name` text,   `breed` text,   `weight` double DEFAULT NULL,   `color` text,   `age` int DEFAULT NULL );
